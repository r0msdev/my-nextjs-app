{
	"servers": {
		"microsoft/playwright-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@playwright/mcp@latest",
				"--browser",
				"msedge"
			]
		},
		"mui-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@mui/mcp@latest"
			]
		},
		"Vercel": {
			"url": "https://mcp.vercel.com",
			"type": "http"
		}
	},
	"inputs": []
}